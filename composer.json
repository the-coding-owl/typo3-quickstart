{
    "name": "the-coding-owl/typo3-quickstart",
    "description": "A composer package to quickstart TYPO3 projects. It comes equipped with an autoinstaller and a Vagrant configuration, so the machine has just to be started to begin development.",
    "type": "project",
    "license": "GPL3v3",
    "authors": [
        {
            "name": "Kevin Ditscheid",
            "email": "kevinditscheid@gmail.com"
        }
    ],
    "require": {
        "typo3/cms-core": "~8.7.8",
        "typo3/cms-belog": "~8.7.8",
        "typo3/cms-beuser": "~8.7.8",
        "typo3/cms-context-help": "~8.7.8",
        "typo3/cms-lowlevel": "~8.7.8",
        "typo3/cms-reports": "~8.7.8",
        "typo3/cms-setup": "~8.7.8",
        "helhum/typo3-console": "^5.0",
        "helhum/typo3-secure-web": "^0.2.5",
        "helhum/typo3-config-handling": "^0.2.1",
        "helhum/dotenv-connector": "^2.0@dev"
    },
    "extra": {
        "helhum/typo3-config-handling": {
            "ext-config": "conf/config.extension.yml"
        },
        "helhum/typo3-console": {
            "install-extension-dummy": false
        },
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": "web",
            "root-dir": "private"
        }
    },
    "repositories": {
        "typo3-autoinstaller": {
            "type": "path",
            "url": "tmp-extensions/typo3-autoinstaller"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "cpsit/mask-export": "dev-master",
        "friendsoftypo3/extension-builder": "dev-master"
    }
}
